<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ita_pontos_ranked"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary_light"
    tools:context="phpmysql.example.com.suecada.ItaPontosActivity">

    <ToggleButton

        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:id="@+id/tBJ1R"
        android:tag="j1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtPontuacaoR"
        android:layout_marginTop="32dp"
        android:checked="false"
        android:textColor="@color/primary_text"
        android:background="@color/icons" />

    <ToggleButton

        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:id="@+id/tBJ2R"
        android:tag="j2"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tBJ1R"
        android:layout_marginTop="8dp" />

    <ToggleButton

        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:id="@+id/tBJ3R"
        android:tag="j3"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tBJ2R"
        android:layout_marginTop="8dp" />

    <ToggleButton

        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:id="@+id/tbJ4R"
        android:tag="j4"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tBJ3R"
        android:layout_marginTop="8dp" />

    <ToggleButton

        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:id="@+id/tBJ5R"
        android:tag="j5"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tbJ4R"
        android:layout_marginTop="8dp" />

    <TextView
        android:text="@string/suecaItaliana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtPontuacaoR"
        android:textSize="36sp"
        android:gravity="center"
        android:textStyle="normal|bold|italic"
        android:textAlignment="center"
        android:textColor="@color/primary_text"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="16dp" />

    <EditText
        android:text="EditText"
        android:clickable="false"
        android:cursorVisible="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:inputType="none"
        android:textColor="@color/icons"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:id="@+id/tVJ1R"
        tools:ignore="HardcodedText,LabelFor"
        app:layout_constraintLeft_toRightOf="@+id/tBJ1R"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintTop_toTopOf="@+id/tBJ1R"
        android:textStyle="normal|bold" />

    <EditText
        android:text="EditText"
        android:clickable="false"
        android:cursorVisible="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:inputType="none"
        android:textColor="@color/icons"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:id="@+id/tVJ2R"
        app:layout_constraintLeft_toRightOf="@+id/tBJ2R"
        app:layout_constraintTop_toBottomOf="@+id/tVJ1R"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp"
        tools:ignore="HardcodedText,LabelFor"
        android:textStyle="normal|bold"
        android:textAllCaps="false" />

    <EditText
        android:text="EditText"
        android:clickable="false"
        android:cursorVisible="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:inputType="none"
        android:textColor="@color/icons"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:id="@+id/tVJ3R"
        app:layout_constraintLeft_toRightOf="@+id/tBJ3R"
        app:layout_constraintTop_toBottomOf="@+id/tVJ2R"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp"
        tools:ignore="HardcodedText,LabelFor"
        android:textStyle="normal|bold" />

    <EditText
        android:text="EditText"
        android:clickable="false"
        android:cursorVisible="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:inputType="none"
        android:textColor="@color/icons"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:id="@+id/tVJ4R"
        app:layout_constraintLeft_toRightOf="@+id/tbJ4R"
        app:layout_constraintTop_toBottomOf="@+id/tVJ3R"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp"
        tools:ignore="HardcodedText,LabelFor"
        android:layout_marginTop="8dp"
        android:textStyle="normal|bold" />

    <EditText
        android:text="EditText"
        android:clickable="false"
        android:cursorVisible="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:inputType="none"
        android:textColor="@color/icons"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:id="@+id/tVJ5R"
        app:layout_constraintLeft_toRightOf="@+id/tBJ5R"
        app:layout_constraintTop_toBottomOf="@+id/tVJ4R"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp"
        tools:ignore="HardcodedText,LabelFor"
        android:textStyle="normal|bold" />

    <Button
        android:text="@string/mais1"
        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn1R"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tBJ5R"
        android:layout_marginTop="24dp"
        />

    <Button
        android:text="@string/mais2"
        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn2R"
        app:layout_constraintLeft_toRightOf="@+id/btn1R"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tBJ5R"
        android:layout_marginTop="24dp"
        app:layout_constraintRight_toLeftOf="@+id/btn4R"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp" />

    <Button
        android:text="@string/mais4"
        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn4R"
        app:layout_constraintTop_toBottomOf="@+id/tBJ5R"
        android:layout_marginTop="24dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="16dp" />

    <Button
        android:text="@string/menos1"
        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnMenos1R"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn1R" />

    <Button
        android:text="@string/menos2"
        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnMenos2R"
        app:layout_constraintLeft_toRightOf="@+id/btnMenos1R"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn2R"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/btnMenos4R"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp" />

    <Button
        android:text="@string/menos4"
        android:background="@color/icons"
        android:textColor="@color/primary_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnMenos4R"
        app:layout_constraintTop_toBottomOf="@+id/btn4R"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="16dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnSairIR"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:textColor="@color/primary_text"
        android:background="@color/icons"
        android:text="@string/terminar"
        android:onClick="sair"
        />

    <Button
        android:text="@string/anular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnAnularItaR"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:textColor="@color/primary_text"
        android:background="@color/icons"/>

    <Button
        android:text="@string/reiniciar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnReiniciarItaR"
        android:background="@color/icons"
        android:textColor="@color/primary_text"
        app:layout_constraintRight_toLeftOf="@+id/btnAnularItaR"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/btnSairIR"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"/>

    <Button
        android:id="@+id/btnTerminarIR"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/ic_stat_terminar"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
